            <ToggleButton ToolTip="Hiển thị riêng 7 kênh quan sát trong СО - thời gian «водопада» (thác nước) &#013; khoảng 40s trong mỗi kênh hiển thị (mặc định)" x:Name="razd" Content="РАЗД" HorizontalAlignment="Left" Width="40" Margin="260,16,0,0" VerticalAlignment="Top" Checked="razd_Checked" Cursor="Hand"/>
            <ToggleButton ToolTip="Hiển thị phối hợp 7 kênh quan sát trong СО – thời gian водопада khoảng 280s" x:Name="sovm" Content="СОВМ" HorizontalAlignment="Left" Width="40" Margin="303,16,0,0" VerticalAlignment="Top" Checked="sovm_Checked" Cursor="Hand"/>
            <ToggleButton ToolTip="Cự ly 300 m" x:Name="r300co" Content="300" HorizontalAlignment="Left" Width="40" Margin="218,43,0,0" VerticalAlignment="Top" Checked="r300co_Checked" Cursor="Hand"/>
            <ToggleButton ToolTip="Cự ly 200 m" x:Name="r200co" Content="200" HorizontalAlignment="Left" Width="40" Margin="260,43,0,0" VerticalAlignment="Top" Checked="r200co_Checked" Cursor="Hand"/>
            <ToggleButton ToolTip="Cự ly 100 m" x:Name="r100co" Content="100" HorizontalAlignment="Left" Width="40" Margin="303,43,0,0" VerticalAlignment="Top" Checked="r100co_Checked" Cursor="Hand"/>
            <Label Content="Круг.обзор" HorizontalAlignment="Left" Margin="72,9,0,0" VerticalAlignment="Top" FontSize="14" Foreground="White">
                <Label.ToolTip>
                    <ToolTip Placement="Top">Điều khiển cửa sổ quan sát ИКО</ToolTip>
                </Label.ToolTip>
            </Label>
            <ToggleButton ToolTip="Chuyển đổi tọa độ tròn sang tọa độ vuông góc" Content="Вид" HorizontalAlignment="Left" Width="40" Margin="158,16,0,0" VerticalAlignment="Top" Cursor="Hand" Height="19" Checked="ToggleButton_Checked" Unchecked="ToggleButton_Unchecked"/>
            <ToggleButton ToolTip="Cự ly 600 m" x:Name="r600" Content="600" HorizontalAlignment="Left" Width="40" Margin="72,43,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.782,0.573" Checked="r600_Checked" Cursor="Hand"/>
            <ToggleButton ToolTip="Cự ly 300 m" x:Name="r300" Content="300" HorizontalAlignment="Left" Width="40" Margin="115,43,0,0" VerticalAlignment="Top" Checked="r300_Checked" Cursor="Hand"/>
            <ToggleButton ToolTip="Cự ly 150 m" x:Name="r150" Content="150" HorizontalAlignment="Center" Width="40" Margin="158,43,162,0" VerticalAlignment="Top" Checked="r150_Checked" Cursor="Hand"/>


            <Button  Content="Инд" HorizontalAlignment="Left" Width="48" Margin="11,16,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.782,0.573" Cursor="Hand" Height="22">
                <Button.Style>
                    <Style TargetType="Button">
                        <Setter Property="Background" Value="Black"></Setter>
                        <Setter Property="Foreground" Value="#FF7FC5FF"></Setter>
                        <Setter Property="BorderBrush" Value="#FF7FC5FF" />
                        <Setter Property="BorderThickness" Value="2"/>
                        <Setter Property="Template">
                            <Setter.Value>
                                <ControlTemplate TargetType="{x:Type Button}">
                                    <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
                                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                    </Border>
                                </ControlTemplate>
                            </Setter.Value>
                        </Setter>
                        <Style.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter Property="Foreground" Value="SpringGreen" />
                                <Setter Property="BorderThickness" Value="3"/>
                                <Setter Property="FontWeight" Value="Bold"/>
                                <Setter Property="BorderBrush" Value="SpringGreen"/>
                                <Setter Property="Background" Value="Black"/>
                            </Trigger>
                            <Trigger Property="IsPressed" Value="True">
                                <Setter Property="Background" Value="Black"></Setter>
                                <Setter Property="Foreground" Value="#FF7FC5FF"></Setter>
                                <Setter Property="BorderBrush" Value="#FF7FC5FF" />
                                <Setter Property="BorderThickness" Value="2"/>
                            </Trigger>
                        </Style.Triggers>
                    </Style>
                </Button.Style>
                <Button.ToolTip>
                    <ToolTip Placement="Top">Điều chỉnh độ sáng tối, mờ nhạt của màn hình</ToolTip>
                </Button.ToolTip>
            </Button>

