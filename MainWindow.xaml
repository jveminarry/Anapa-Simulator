                <Border.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform AngleX="0.106"/>
                        <RotateTransform/>
                        <TranslateTransform X="0.048"/>
                    </TransformGroup>
                </Border.RenderTransform>
            </Border>
            <Border BorderBrush="#FFD4D4D4" BorderThickness="1.5" HorizontalAlignment="Left" Height="58" Margin="207,0,0,0" VerticalAlignment="Center" Width="141"/>
