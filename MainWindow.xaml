            <ToggleButton ToolTip="-30 dB" x:Name="m30db" Content="-30" HorizontalAlignment="Left" Width="40" Margin="304,15,0,0" VerticalAlignment="Top" Checked="m30db_Checked"/>
            <Label Content="Очист." HorizontalAlignment="Left" Margin="31,34,0,0" VerticalAlignment="Top" Foreground="White" FontSize="14">
