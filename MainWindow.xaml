            <Label x:Name="Dateandtime" HorizontalAlignment="Left" Margin="750,0,0,2.2" Height="28" Width="205" Foreground="#FFFCF9F9" FontSize="14" Content="" FontWeight="Bold" VerticalAlignment="Center">
                <Label.ToolTip>
                    <ToolTip>Thời gian thực</ToolTip>
                </Label.ToolTip>
            </Label>

