                </ToggleButton.ToolTip>
            </ToggleButton>
            <ToggleButton x:Name="akshavt" Content="АВТ" HorizontalAlignment="Left" Width="40" Margin="261,39,0,0" VerticalAlignment="Top" Checked="akshavt_Checked" Unchecked="akshavt_Unchecked">
