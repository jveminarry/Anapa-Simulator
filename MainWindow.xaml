                    <ToolTip Placement="Left">
                        <TextBlock>Điều chỉnh mức công suất phát !</TextBlock>
                    </ToolTip>
                </Label.ToolTip>
            </Label>
            <ToggleButton ToolTip="Tắt phát" x:Name="pwokl" Content="ОТКЛ" HorizontalAlignment="Left" Width="40" Margin="172,12,0,0" VerticalAlignment="Top" Checked="pwokl_Checked" Cursor="Hand" />
            <ToggleButton ToolTip="50% công suất định mức" x:Name="pw50" Content="50%" HorizontalAlignment="Left" Width="40" Margin="217,12,0,0" VerticalAlignment="Top" Checked="pw50_Checked" Cursor="Hand"/>
            <ToggleButton ToolTip="75% công suất định mức" x:Name="pw75" Content="75%" HorizontalAlignment="Left" Width="40" Margin="260,12,0,0" VerticalAlignment="Top" Checked="pw75_Checked" Cursor="Hand" />
            <ToggleButton ToolTip="100% công suất định mức" x:Name="pw100" Content="100%" HorizontalAlignment="Left" Width="40" Margin="303,12,0,0" VerticalAlignment="Top" Checked="pw100_Checked" Cursor="Hand"/>
            <Label Content="Угон УХН" HorizontalAlignment="Left" Margin="47,31,0,0" VerticalAlignment="Top" Foreground="White" FontSize="14">
                <Label.ToolTip>
                    <ToolTip Placement="Left">Điều chỉnh góc lệch cánh sóng hẹp</ToolTip>
                </Label.ToolTip>
            </Label>
            <TextBox ToolTip="Giá trị này chỉ khả dụng khi chọn búp sóng hẹp, giá trị trong khoảng -5 đến 5°" x:Name="uhn" HorizontalAlignment="Left" Width="40" Margin="116,36,0,0" VerticalAlignment="Top" TextAlignment="Center" IsEnabled="False" Text="0" />
            <ToggleButton ToolTip="Hẹp: xấp xỉ 7°" x:Name="smallbw" Content="УХН" HorizontalAlignment="Left" Width="40" Margin="217,36,0,0" VerticalAlignment="Top" Checked="smallbw_Hit" Height="19" Cursor="Hand" Unchecked="smallbw_Unchecked" />
            <ToggleButton ToolTip="Vừa: xấp xỉ 11°" x:Name="mediumbw" Content="СХН" HorizontalAlignment="Left" Width="40" Margin="260,36,0,0" VerticalAlignment="Top" Checked="mediumbw_Checked" Height="19" Cursor="Hand"/>
            <ToggleButton ToolTip="Rộng: xấp xỉ 20°" x:Name="largebw" Content="ШХН" HorizontalAlignment="Left" Width="40" Margin="303,36,0,13" Checked="largebw_Checked" Cursor="Hand"/>
            <Label Content="Тип ХН" HorizontalAlignment="Left" Margin="161,31,0,0" VerticalAlignment="Top" FontSize="14" Foreground="White">
                <Label.ToolTip>
                    <ToolTip Placement="Left">Loại cánh sóng</ToolTip>
                </Label.ToolTip>
            </Label>
        </Grid>

        <!--Bảng điều khiển hệ số khuếch đại, độ bù trôi dạt-->

