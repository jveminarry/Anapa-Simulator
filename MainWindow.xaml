            <Grid.Resources>
                <Style TargetType="{x:Type ToggleButton}">
                    <Setter Property="Template">
                        <Setter.Value>
                            <ControlTemplate TargetType="{x:Type ToggleButton}">
                                <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
                                    <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                </Border>
                            </ControlTemplate>
                        </Setter.Value>
                    </Setter>
                    <Style.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Background" Value="Black"/>
                            <Setter Property="BorderBrush" Value="SpringGreen"/>
                            <Setter Property="Foreground" Value="SpringGreen"/>
                            <Setter Property="BorderThickness" Value="2"/>
                            <Setter Property="Cursor" Value="Hand"/>
                        </Trigger>
                        <Trigger Property="IsChecked" Value="True">
                            <Setter Property="Background" Value="Black"/>
                            <Setter Property="BorderBrush" Value="SpringGreen"/>
                            <Setter Property="Foreground" Value="SpringGreen"/>
                            <Setter Property="BorderThickness" Value="2"/>
                        </Trigger>
                    </Style.Triggers>
                </Style>
            </Grid.Resources>
            <Border BorderThickness="2" BorderBrush="White" Margin="3,4,2,8"/>
            <Label Content="Усиление АПО, дБ" HorizontalAlignment="Left" Margin="31,10,0,0" VerticalAlignment="Top" FontSize="14" Foreground="White">
                <Label.ToolTip>
                    <ToolTip Placement="Left">Điều chỉnh mức khuếch đại bằng tay</ToolTip>
                </Label.ToolTip>
            </Label>
            <ToggleButton ToolTip="0 dB" x:Name="_0db" Content="0" HorizontalAlignment="Left" Width="40" Margin="175,15,0,0" VerticalAlignment="Top" Checked="_0db_Checked"/>
            <ToggleButton ToolTip="-10 dB (thường chọn)" x:Name="m10db" Content="-10" HorizontalAlignment="Left" Width="40" Margin="218,15,0,0" VerticalAlignment="Top" Checked="m10db_Checked"/>
