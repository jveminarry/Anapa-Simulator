            <ToggleButton ToolTip="Tắt (mặc định)" x:Name="noiseotkl" Content="ОТКЛ" HorizontalAlignment="Left" Width="40" Margin="82,39,0,0" VerticalAlignment="Top" Checked="noiseotkl_Checked"/>
