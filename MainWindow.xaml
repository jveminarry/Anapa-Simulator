            <TextBlock ToolTip="Thứ tự mục tiêu" Height="14" TextWrapping="Wrap" Text="No" VerticalAlignment="Center" Background="Black" Foreground="White" TextAlignment="Center" FontSize="8" FontWeight="Bold" Margin="-1,0,316,0"/>
