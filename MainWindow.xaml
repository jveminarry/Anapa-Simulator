                    <ToolTip Placement="Top">Chọn bằng tay các vật thể cố định (không khả dụng)</ToolTip>
                </ToggleButton.ToolTip>
            </ToggleButton>
            <Label Content="АКСН" HorizontalAlignment="Left" Margin="172,34,0,0" VerticalAlignment="Top" FontSize="14" Foreground="White">
                <Label.ToolTip>
                    <ToolTip Placement="Top">Điều khiển các chế độ làm việc tự động bù trôi dạt tàu mang</ToolTip>
                </Label.ToolTip>
            </Label>
            <ToggleButton ToolTip="Bật" x:Name="noisevkl" Content="ВКЛ" HorizontalAlignment="Left" Width="40" Margin="127,39,0,0" VerticalAlignment="Top" Checked="noisevkl_Checked"/>
        </Grid>

